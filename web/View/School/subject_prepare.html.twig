{% extends './base.html.twig' %}

{% block title %}eDziennik AMN - klasy{% endblock %}

{% block content %}
<div class="row" style="margin-top:60px;margin-bottom: 20px;">
    <div class="panel panel-primary">
        <div class="panel-heading">
            <h3 class="panel-title"><strong>Nowy przedmiot</strong></h3>
        </div>
        <div class="panel-body">
            <form name="inputform" method="POST" action="/admin/school/subjects/add">
                <div class="input-group">
                    <span class="input-group-addon" >Nazwa przedmiotu:</span>
                    <input type="text" name="name" value="" class="form-control" style="width:250px;" />

                    
                    <input type="submit" name="save" value="Zapisz" class="btn btn-success" />
                </div>

            </form>
        </div>
    </div>
</div>

{% endblock %}
